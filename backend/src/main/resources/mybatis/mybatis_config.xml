<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- <settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings> -->
	<typeAliases>
		<!-- 사용할 DTO들을 예약어로 등록 -->
		
		<typeAlias type="com.ssafy.happyhouse.dto.HouseInfo" alias="houseinfo"/>
		<typeAlias type="com.ssafy.happyhouse.dto.HouseInfoDTO" alias="houseinfodto"/>
		<typeAlias type="com.ssafy.happyhouse.dto.UserInfo" alias="user"/>
		<typeAlias type="com.ssafy.happyhouse.dto.SidoCodeDTO" alias="sidocode"/>
		<typeAlias type="com.ssafy.happyhouse.dto.SearchInfo" alias="searchinfo"/>
		<typeAlias type="com.ssafy.happyhouse.dto.QnA" alias="qna"/>
		<typeAlias type="com.ssafy.happyhouse.dto.Notice" alias="notice"/>
		<typeAlias type="com.ssafy.happyhouse.dto.PreferArea" alias="preferarea"/>
		<typeAlias type="com.ssafy.happyhouse.dto.HouseDeal" alias="housedeal"/>
		
		
	</typeAliases>

	<mappers>
		<!-- 사용할 mapper 등록 -->
		<mapper resource="mybatis/mapper/userMapper.xml" />
		<mapper resource="mybatis/mapper/houseInfoMapper.xml" />
		<mapper resource="mybatis/mapper/selectBoxMapper.xml" />
		<mapper resource="mybatis/mapper/qnaMapper.xml" />
		<mapper resource="mybatis/mapper/noticeMapper.xml" />
		<mapper resource="mybatis/mapper/preferAreaMapper.xml" />
		<mapper resource="mybatis/mapper/deal.xml" />
	</mappers>
</configuration>